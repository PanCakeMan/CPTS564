SOURCE_DIR = 

TARGET_ARCH=x64Darwin15clang7.0

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := /Applications/rti_connext_dds-5.3.0
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./Breakdown.java $(SOURCE_DIR)./BreakdownSeq.java $(SOURCE_DIR)./BreakdownTypeSupport.java $(SOURCE_DIR)./BreakdownTypeCode.java $(SOURCE_DIR)./BreakdownDataReader.java $(SOURCE_DIR)./BreakdownDataWriter.java $(SOURCE_DIR)./Accident.java $(SOURCE_DIR)./AccidentSeq.java $(SOURCE_DIR)./AccidentTypeSupport.java $(SOURCE_DIR)./AccidentTypeCode.java $(SOURCE_DIR)./AccidentDataReader.java $(SOURCE_DIR)./AccidentDataWriter.java $(SOURCE_DIR)./Position.java $(SOURCE_DIR)./PositionSeq.java $(SOURCE_DIR)./PositionTypeSupport.java $(SOURCE_DIR)./PositionTypeCode.java $(SOURCE_DIR)./PositionDataReader.java $(SOURCE_DIR)./PositionDataWriter.java $(SOURCE_DIR)./Vehicle.java $(SOURCE_DIR)./VehicleSeq.java $(SOURCE_DIR)./VehicleTypeSupport.java $(SOURCE_DIR)./VehicleTypeCode.java $(SOURCE_DIR)./VehicleDataReader.java $(SOURCE_DIR)./VehicleDataWriter.java $(SOURCE_DIR)./VehicleSubscriber.java $(SOURCE_DIR)./VehiclePublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".:$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export DYLD_LIBRARY_PATH := $(NDDSHOME)/lib/x64Darwin15clang7.0:/usr/lib/lwp:$(DYLD_LIBRARY_PATH)

VehiclePublisher: ./VehiclePublisher.class
	$(JAVA_PATH) -d64 -Djava.library.path=$(NDDSHOME)/lib/x64Darwin15clang7.0 -classpath ".:$(RTI_CLASSPATH)" VehiclePublisher $(ARGS)
 
VehicleSubscriber: ./VehicleSubscriber.class
	$(JAVA_PATH) -d64 -Djava.library.path=$(NDDSHOME)/lib/x64Darwin15clang7.0 -classpath ".:$(RTI_CLASSPATH)" VehicleSubscriber $(ARGS)
